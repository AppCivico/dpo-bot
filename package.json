{
  "name": "dpo-bot",
  "version": "1.1.0",
  "private": true,
  "scripts": {
    "start:bottender": "babel-node server.js",
    "start:proxy": "babel-node proxy/server.js",
    "dev": "nodemon server.js --exec babel-node",
    "test": "jest --detectOpenHandles",
    "pm2_add": "pm2 start app/index.js  --name \"dpo-homol\"",
    "restart": "pm2 restart \"dpo-homol\" --update-env",
    "logs": "pm2 logs \"dpo-homol\" --lines 1000",
    "webhook": "npx bottender messenger webhook set",
    "profileGet": "npx bottender messenger profile get",
    "profileSet": "npx bottender messenger profile set",
    "react:dev": "react-scripts start --dev",
    "react:start": "react-scripts start",
    "react:build": "react-scripts build",
    "react:test": "react-scripts test",
    "react:eject": "react-scripts eject",
    "react:serve": " serve -s build -p 1991 ",
    "react:pm2": "pm2 start --name dpo-browser  npm -- run react:serve",
    "bottender:build": "docker-compose up -d --build bottender",
    "bottender:log": "docker logs dpo-bottender -f",
    "bottender:pm2": "pm2 start --name dpo-bottender  npm -- run start:bottender",
    "browser:build": "docker-compose up -d --build browser",
    "browser:log": "docker logs dpo-browser -f",
    "proxy:build": "docker-compose up -d --build proxy",
    "proxy:log": "docker logs dpo-proxy -f",
    "proxy:pm2": "pm2 start --name dpo-proxy  npm -- run start:proxy",
    "redis:build": "docker-compose up -d --build redis",
    "redis:log": "docker logs dpo-redis -f"
  },
  "dependencies": {
    "@babel/cli": "^7.10.3",
    "@babel/core": "^7.10.3",
    "@babel/node": "^7.10.3",
    "@babel/plugin-proposal-class-properties": "^7.10.1",
    "@babel/plugin-transform-runtime": "^7.10.3",
    "@babel/preset-env": "^7.10.3",
    "@chentsulin/react-botui": "^0.2.1",
    "@sentry/node": "^5.15.4",
    "async-child-process": "^1.1.1",
    "axios": "^0.19.2",
    "body-parser": "^1.19.0",
    "bottender": "^1.3.5",
    "botui": "^0.3.9",
    "child_process": "^1.0.2",
    "cors": "^2.8.5",
    "cron": "^1.8.2",
    "customize-cra": "^0.9.1",
    "dialogflow": "^1.2.0",
    "dotenv": "^4.0.0",
    "express": "^4.17.1",
    "file-type": "^12.3.0",
    "fs-extra": "^7.0.0",
    "get-audio-duration": "0.0.1",
    "googleapis": "^51.0.0",
    "grpc": "^1.24.2",
    "itsa-surveymonkey-promise": "0.0.6",
    "jest": "^25.3.0",
    "jsonfile": "^5.0.0",
    "jsonwebtoken": "^8.5.1",
    "linkifyjs": "^2.1.9",
    "messaging-api-messenger": "^0.7.16",
    "moment": "^2.24.0",
    "node-fetch": "^2.6.0",
    "nodemailer": "^6.4.2",
    "nodemon": "^1.19.1",
    "promisepipe": "^3.0.0",
    "rand-token": "^1.0.1",
    "react": "^16.13.1",
    "react-app-rewired": "^2.1.6",
    "react-dev-utils": "^10.2.1",
    "react-dom": "^16.13.1",
    "react-scripts": "^3.4.1",
    "redis": "^3.0.2",
    "remove-accents": "^0.4.2",
    "request": "^2.88.0",
    "request-promise": "^4.2.2",
    "requisition": "^1.7.0",
    "sanitize-html": "^1.26.0",
    "surveymonkey": "^1.1.0",
    "util": "^0.12.1",
    "util.promisify": "^1.0.0",
    "uuid": "^8.1.0",
    "validar-cpf": "^2.1.1"
  },
  "devDependencies": {
    "eslint": "^6.2.1",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-plugin-import": "^2.18.2"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
